{
  "name": "drag-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "jest": {
    "maxWorkers": 1,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@drag/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "check:format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "check:lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"src/**/*.{js,mjs,cjs,ts,mts}\"",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --maxWorkers=1",
    "pre-commit": "lint-staged",
    "lint:fix": "eslint \"src/**/*.{js,mjs,cjs,ts,mts}\" --fix"
  },
  "dependencies": {
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/swagger": "^7.1.6",
    "@nestjs/typeorm": "^10.0.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "fastify": "^4.21.0",
    "fastify-cookie": "^5.7.0",
    "fastify-cors": "^6.1.0",
    "fastify-swagger": "^5.2.0",
    "googleapis": "^123.0.0",
    "joi": "^17.9.2",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/prompt-cli": "^17.6.7",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-kit": "^10.4.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "18.*"
  }
}
