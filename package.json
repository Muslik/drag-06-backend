{
  "name": "drag-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "check:lint": "eslint \"{src,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "test": "cross-env NODE_ENV=test jest --config jest_config/jest.unit.config.ts",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config jest_config/jest.unit.config.ts",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --config jest_config/jest.unit.config.ts",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand  --config jest_config/jest.unit.config.ts",
    "test:e2e": "cross-env NODE_ENV=test jest --maxWorkers=1",
    "pre-commit": "lint-staged",
    "lint:fix": "eslint \"src/**/*.{js,mjs,cjs,ts,mts}\" --fix"
  },
  "dependencies": {
    "@fastify/cookie": "^9.2.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/static": "^6.12.0",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-fastify": "^10.2.10",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "@sweet-monads/either": "^3.3.1",
    "@sweet-monads/maybe": "^3.3.1",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/uuid": "^9.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "fastify": "4.25.1",
    "fastify-cookie": "^5.7.0",
    "fastify-cors": "^6.1.0",
    "fastify-swagger": "^5.2.0",
    "googleapis": "^129.0.0",
    "joi": "^17.11.0",
    "nanoid": "^5.0.4",
    "nestjs-i18n": "^10.4.0",
    "nestjs-request-context": "^3.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/prompt-cli": "^18.4.3",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-kit": "^10.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nestjs-spelunker": "^1.3.0",
    "prettier": "^3.1.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
